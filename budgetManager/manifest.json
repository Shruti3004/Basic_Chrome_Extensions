{
    "name": "Budget Manager",
    "description" : "This extension tracks your overall spendings",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "budget.html",
        "default_icon": "favicon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "storage",
        "notifications"
    ]
}
